import java.util.Scanner;
public class Solution {
    
    static void printAllPrime(int n)
    {
		for(int i = 2; i <= n; i++)
        {
            for(int j = 2; j <= n; j++)
            {
                if(i % j == 0)
                    System.out.println(j);

            }
        }
    }

	public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int n = sc .nextInt();
        
        printAllPrime(n);
	}
}
